{
  "extends": ["adpyke-es6", "prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": false,
    "mocha": true,
    "jquery": true
  },
  "rules": {
    "func-style": "off",
    "no-invalid-this": "off"
  },
  "globals": {
    "Tesseract": true
  }
}
